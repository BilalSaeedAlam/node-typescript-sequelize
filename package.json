{
  "name": "node-typescript-sequelize",
  "version": "1.0.0",
  "description": "",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "build": "tsc",
    "migrate": "yarn build && sequelize-cli db:migrate",
    "migrate:undo": "yarn build && sequelize-cli db:migrate:undo",
    "migrate:undo:all": "yarn build && sequelize-cli db:migrate:undo:all",
    "seed": "yarn build && sequelize-cli db:seed:all",
    "seed:undo": "yarn build && sequelize-cli db:seed:undo",
    "seed:undo:all": "yarn build && sequelize-cli db:seed:undo:all",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "jest"
  },
  "keywords": [],
  "author": "Bilal Saeed Alam",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.5.2",
    "@types/sequelize": "^4.28.20",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
